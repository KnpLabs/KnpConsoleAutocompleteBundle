{
    "name":        "knplabs/knp-console-autocomplete-bundle",
    "type":        "symfony-bundle",
    "description": "This Symfony2 bundle provides a simple way of autocompleting your commands in your shell.",
    "keywords":    ["console", "autocomplete", "knplabs", "knp", "bundle"],
    "homepage":    "http://github.com/KnpLabs/KnpConsoleAutocompleteBundle",
    "license":     "MIT",

    "authors": [
        {
            "name": "KnpLabs Team",
            "homepage": "http://knplabs.com"
        },
        {
            "name": "Symfony Community",
            "homepage": "http://github.com/KnpLabs/KnpConsoleAutocompleteBundle/contributors"
        }
    ],

    "require": {
        "php":             ">=5.3.2",
        "symfony/console": ">=2.0.0"
    },

    "autoload": {
        "psr-0": {
            "Knp\\Bundle\\ConsoleAutocompleteBundle": ""
        }
    },

    "target-dir": "Knp/Bundle/ConsoleAutocompleteBundle"
}
